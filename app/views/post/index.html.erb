<div class="row">
  <div class="col-sm-2">
  </div>
  <div class="col-sm-9">
    <div class="row">
      <div class="col-sm-1">
      </div>
      <div class="col-sm-7">
        <% @posts.each do |post|%>
          <%# if current_user.followed?(post.user.id)%>
          <%= render partial: 'layouts/postcard', locals: {post: post}%> 
          <div class="fence"></div>
        <%end%>   
      </div>
      <div class="col-sm-3">
        <%= render partial: 'layouts/usersection', locals: {post: @posts, users: @users} %>
      </div>
    </div>
  </div>
  <div class="col-sm-1">
  </div>
</div>